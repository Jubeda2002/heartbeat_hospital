<%-include("navbar.ejs"); %>
<div class="row">
    <div class="col-12">
        <form action="insurance_details" method="post" class="insurance_details" enctype="multipart/form-data">
           
           <h5>
            Insurance Title
            </h5>
             <input type="text" class="form-control" name="insurance_title" placeholder=" Insurance Title">
             <br>
           
           <button class="btn btn-lg btn-primary" type="submit">Submit</button>
    </form>

<br>
<table class="table table-striped">
    <thead>
        <tr>
            <th>Insurance ID</th>
         
            <th>Insurance Title</th>
       
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        <% insurance.map(function(row,i){ %>
        <tr>
            <td><%= i+1 %></td>
          
            <td><%= row.insurance_title %></td>
          
           <td>
                <td>
                    
                    <a href="/admin/insurance_delete/<%= row.insurance_id %>" class="btn btn-danger" 
                       onclick="return confirm('Are you sure you want to delete this insurance?');">
                       Delete
                    </a>
                </td>
                
            </td>
        </tr>
        <% }) %>
    </tbody>
</table>
</div>
</div>
<%-include("footer.ejs"); %>

