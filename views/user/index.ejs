<div class="container">
  <h1>Hospital Capabilities Growth Over the Years</h1>
  <div class="stats">
      <div class="stat-box">
          <div class="stat-number" id="patients-count">0</div>
          <p>Patients Treated</p>
      </div>
      <div class="stat-box">
          <div class="stat-number" id="departments-count">0</div>
          <p>Specialized Departments</p>
      </div>
      <div class="stat-box">
          <div class="stat-number" id="surgeries-count">0</div>
          <p>Successful Surgeries</p>
      </div>
  </div></div>

  <script>
    function animateCounter(id, start, end, duration) {
            let obj = document.getElementById(id);
            let range = end - start;
            let current = start;
            let increment = range / (duration / 10);
            
            let timer = setInterval(() => {
                current += increment;
                obj.textContent = Math.floor(current);
                if (current >= end) {
                    obj.textContent = end;
                    clearInterval(timer);
                }
            }, 10);
        }

        animateCounter("patients-count", 0, 50000, 2000);
        animateCounter("departments-count", 0, 15, 2000);
        animateCounter("surgeries-count", 0, 12000, 2000);

    </script>

